{
  "name": "Streamie",
  "description": "Streamie is a realtime Twitter client that runs right in your browser.",
  "version": "3",
  "app": {
    "urls": [
      "*://streamie.com/"
    ],
    "launch": {
      "web_url": "http://streamie.com:8888/"
    }
  },
  "icons": {
    "128": "streamie-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
		"background"
  ]
}
